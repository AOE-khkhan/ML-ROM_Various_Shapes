{
    "GPU": {
        "number": "1",
        "multi_gpu": false,
        "gpu_count": 1
    },
    "save": {
        "val_time_save": false,
        "history_save": true
    },
    "data_loader": {
        "dataset_name": "72_values_MS-BN-1_dataset.csv",
        "num_of_ts": 500,
        "sequence": 400,
        "num_of_ts_for_data": 550,
        "kind_num": 80,
        "": "---------------------------------------------------------",
        "": "num_of_ts + time_step * (maxlen - 1) < num_of_ts_for_data",
        "": "---------------------------------------------------------",
        "maxlen": 1,
        "time_step": 1,
        "data_size": 72,
        "path_to_present_dir": "/home/hasegawa/work/Various_shapes_of_bluff_body",
        "with_shape": true,
        "train": true,
        "ratio_tr_te": 0.2
    },
    "model": {
        "name": "LSTM",
        "stateful": true,
        "layer_num": 3,
        "unit_num": [
            128,
            128,
            128
        ],
        "shape_input_layer": 1,
        "optimizer": "adam",
        "": "---- if you use SGD as optimizer ----",
        "lr": 0.01,
        "momentum": 0.0,
        "decay": 0.0,
        "nesterov": false,
        "": "-------------------------------------",
        "act": "tanh",
        "loss": "mse"
    },
    "trainer": {
        "": "--------- cross validation ---------",
        "use_CV": false,
        "n_splits": 4,
        "": "------------------------------------",
        "num_epochs": 30,
        "batch_size": 1
    },
    "callbacks": {
        "": "--------- model checkpoint ---------",
        "modelcheckpoint": true,
        "save_file": "/LSTM/",
        "model_name": "stateful_LSTM_sequence_500",
        "cp_monitor": "val_loss",
        "cp_save_best_only": true,
        "cp_verbose": 1,
        "": "--------- early stopping ---------",
        "earlystopping": false,
        "es_monitor": "val_loss",
        "es_patience": 40,
        "es_verbose": 1,
        "": "--------- tensor borad ---------",
        "tensorboard": false,
        "tensorboard_log_dir": "",
        "tensorboard_write_graph": true,
        "": "--------- reduce LR ---------",
        "reduceLR": false,
        "reduceLR_monitor": "val_loss",
        "": "new_LR = LR * reduceLR_factor",
        "reduceLR_factor": 0.2,
        "reduceLR_patience": 20,
        "reduceLR_verbose": 1
    }
}